
Import("base.dsb>picture.dsf")

function SelectColor(x, y, color)
    pic:Add(x, y, color)
end
function SelectMatchedColor(x, y, color, sim)
    pic:AddEx(x, y, color, true, sim)
end

function 选择颜色(x, y, color)
    pic:Add(x, y, color)
end
function 选择匹配度颜色(x, y, color, sim)
    pic:AddEx(x, y, color, true, sim)
end

